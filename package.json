{
  "name": "creative-meditate",
  "version": "1.0.1",
  "description": "MCP server for creative meditation and insight generation",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "creative-meditate": "dist/index.js"
  },
  "scripts": {
    "clean": "rm -rf dist *.tgz",
    "build": "tsc && npm pack --dry-run",
    "pack": "tsc && npm pack",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "demo": "node dist/demo-client.js",
    "dev:demo": "ts-node src/demo-client.ts",
    "web": "node dist/web/server.js",
    "dev:web": "ts-node src/web/server.ts",
    "test": "vitest run --reporter default",
    "test:watch": "vitest",
    "ci": "pnpm install && pnpm run pack && pnpm test",
    "lint": "eslint --ext .ts,.js .",
    "lint:fix": "eslint --ext .ts,.js . --fix"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.0",
    "axios": "^1.6.0",
    "express": "^4.18.2",
    "zod": "^3.22.0"
  },

  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "mcp", 
    "creativity", 
    "meditation", 
    "insight"
  ],
  "author": "MCP Creative Meditation Contributors",
  "license": "MIT"
}
